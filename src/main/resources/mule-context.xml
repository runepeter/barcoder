<?xml version="1.0" encoding="UTF-8"?>
<mule xmlns="http://www.mulesoft.org/schema/mule/core"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xmlns:spring="http://www.springframework.org/schema/beans"
      xmlns:file="http://www.mulesoft.org/schema/mule/file"
      xmlns:vm="http://www.mulesoft.org/schema/mule/vm"
      xsi:schemaLocation="
      http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
      http://www.mulesoft.org/schema/mule/core http://www.mulesoft.org/schema/mule/core/3.0/mule.xsd
      http://www.mulesoft.org/schema/mule/file http://www.mulesoft.org/schema/mule/file/3.0/mule-file.xsd
      http://www.mulesoft.org/schema/mule/vm http://www.mulesoft.org/schema/mule/vm/3.0/mule-vm.xsd">

  <flow name="create">
    <vm:inbound-endpoint path="create"/>
    <vm:outbound-endpoint path="create-qr"/>
    <vm:outbound-endpoint path="create-datamatrix"/>
  </flow>

  <flow name="create-qr">
    <vm:inbound-endpoint path="create-qr"/>
    <log-component/>
  </flow>

  <flow name="create-datamatrix">
    <vm:inbound-endpoint path="create-datamatrix"/>
    <log-component/>
  </flow>

</mule>
